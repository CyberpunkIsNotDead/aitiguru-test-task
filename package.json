{
  "name": "aitiguru-test-task",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --cache --cache-location .cache/eslint/",
    "lint:fix": "eslint . --cache --cache-location .cache/eslint/ --fix",
    "lint:style": "stylelint \"**/*.{css,scss}\" --cache --cache-location .cache/stylelint/",
    "lint:style:fix": "stylelint \"**/*.{css,scss}\" --cache --cache-location .cache/stylelint/ --fix",
    "lint:all": "npm run lint && npm run lint:style",
    "lint:all:fix": "npm run lint:fix && npm run lint:style:fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "setup-hooks": "chmod +x scripts/setup-hooks.sh && scripts/setup-hooks.sh",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-slot": "^1.2.4",
    "classnames": "^2.5.1",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-compiler": "^1.0.2",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "stylelint": "^17.2.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-order": "^7.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vite-plugin-svgr": "^4.5.0"
  }
}
